# Do NOT modify this file by hand!
upstream graphite {
    server 127.0.0.1:{{ graphite_api_process_bind }} fail_timeout=0;
}

server {
    server_name graph;
    listen {{ graphite_api_bind }} default;
    root /srv/www/graphite;

    location / {
        try_files $uri @graphite;
    }

    location @graphite {
        proxy_pass http://graphite;
    }
}
